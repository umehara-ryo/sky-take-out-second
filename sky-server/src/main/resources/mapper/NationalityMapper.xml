<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.NationalityMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="nationalityCd">
        insert into nationality (nationalityCd,nationalityName)
        values (#{nationalityCd},#{nationalityName})
    </insert>

    <select id="getAll" resultType="com.sky.entity.Nationality">
        select * from nationality;
    </select>
</mapper>
